<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="comp"></include>
			<exclude name="bags"></exclude>
		</run>
	</groups>
	<test thread-count="5" name="Test">
		<classes>
			<class name="laptops.Dell" />
			<class name="laptops.Lenovo" />
			<class name="trolleys.AmericanTourister" />
			<class name="trolleys.Samsonite" />
			<class name="trolleys.WildCraft" />
			<class name="laptops.HP" />
			<class name="trolleys.Skybags" />
			<class name="laptops.Sony" />
			<class name="trolleys.VIP" />
			<class name="laptops.Mac" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
